<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/appboy_feed_swipe_container" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/com_appboy_feed_root" android:layout_width="match_parent" android:layout_height="match_parent">
        <ProgressBar android:id="@+id/com_appboy_feed_loading_spinner" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="15sp" android:layout_centerInParent="true"/>
        <LinearLayout android:orientation="vertical" android:id="@+id/com_appboy_feed_network_error" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="15sp" android:layout_centerInParent="true">
            <TextView android:layout_width="match_parent" android:layout_height="match_parent" style="@style/Appboy.Feed.NetworkErrorTitle"/>
            <TextView android:id="@android:id/empty" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/Appboy.Feed.NetworkErrorBody"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/com_appboy_feed_empty_feed" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="15sp" android:layout_centerInParent="true">
            <TextView android:gravity="center_horizontal" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/Appboy.Feed.Empty"/>
        </LinearLayout>
        <ListView android:id="@android:id/list" android:visibility="gone" android:layout_width="@dimen/com_appboy_feed_max_width" android:layout_height="match_parent" android:layout_centerHorizontal="true" style="@style/Appboy.Feed.List"/>
        <View android:id="@+id/com_appboy_feed_transparent_full_bounds_container_view" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </RelativeLayout>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
