<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" style="@style/Appboy.ContentCards.CaptionedImage">
    <RelativeLayout android:id="@+id/com_appboy_content_cards_captioned_image_card_image_container" style="@style/Appboy.ContentCards.CaptionedImage.ImageContainer"/>
    <ImageView android:id="@+id/com_appboy_content_cards_pinned_icon" style="@style/Appboy.ContentCards.PinnedIcon.CaptionedImage"/>
    <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/Appboy.ContentCards.CaptionedImage.Container">
        <TextView android:id="@+id/com_appboy_content_cards_captioned_image_title" style="@style/Appboy.ContentCards.CaptionedImage.Title"/>
        <TextView android:id="@+id/com_appboy_content_cards_captioned_image_description" style="@style/Appboy.ContentCards.CaptionedImage.Description"/>
        <TextView android:id="@+id/com_appboy_content_cards_action_hint" style="@style/Appboy.ContentCards.ActionHint.CaptionedImage"/>
    </RelativeLayout>
    <View android:id="@+id/com_appboy_content_cards_unread_bar" style="@style/Appboy.ContentCards.UnreadBar.CaptionedImage"/>
</RelativeLayout>
